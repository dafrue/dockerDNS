$TTL	604800
@	IN	SOA	root. admin.root. (
			      5		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
				IN	NS	root.
belag.			IN	NS	ns.belag.

; name servers - A records
root.			IN	A	172.20.10.1
ns.belag.		IN	A	172.20.10.2

; A records

